{
  "conceptual_elements": {
    "agent": {
      "definition": "An agent is a living entity or autonomous system capable of performing actions.",
      "properties": [
        "is_autonomous",
        "has_intent",
        "can_perform_actions",
        "has_attributes"
      ],
      "examples": [...]
    },
    "object": {
      "definition": "A static entity with useful properties, often manipulated by an agent.",
      "properties": [
        "is_physical",
        "has_material",
        "has_function",
        "has_reason",
        "is_manipulable"
      ],
      "examples": [...]
    },
    "action": {
      "definition": "A process executed by an agent with a specific purpose or reason.",
      "properties": [
        "has_initiator",
        "has_purpose",
        "changes_state",
        "has_temporal_context"
      ],
      "examples": [...]
    },
    "motion": {
      "definition": "Something constant, a process already happening, or a state of being.",
      "properties": [
        "is_ongoing",
        "is_process",
        "describes_agent_state"
      ],
      "examples": [...]
    },
    "bridge": {
      "definition": "Relational words that connect conceptual elements and clarify relationships.",
      "properties": [
        "connects_elements",
        "clarifies_causality",
        "indicates_purpose"
      ],
      "examples": [...]
    },
    "properties": {
      "definition": "Foundational, detailed data that provides a profound conceptual understanding of the world.",
      "properties": [
        "is_foundational",
        "provides_causal_context",
        "defines_attributes"
      ],
      "examples": [...]
    },
    "socio-linguistic_context": {
      "definition": "Concepts that describe the social and cultural context of language use.",
      "properties": [
        "tone",
        "style",
        "sentiment",
        "demographic_info"
      ],
      "examples": [
        {
          "phrase": "What's up?",
          "breakdown": {
            "what's up": {
              "element": "socio-linguistic_context",
              "attributes": [
                "style:informal",
                "purpose:greeting"
              ]
            }
          }
        },
        {
          "phrase": "I see the ocean.",
          "breakdown": {
            "I see the ocean": {
              "element": "multimodal_concept",
              "attributes": [
                "modality:visual",
                "state:present"
              ]
            }
          }
        }
      ]
    },
    "multimodal_properties": {
      "definition": "Properties that can be described across different modalities (e.g., text, visual, audio).",
      "properties": [
        "color",
        "shape",
        "texture",
        "pitch",
        "tempo"
      ],
      "examples": [
        {
          "modality": "visual",
          "description": "a red car",
          "breakdown": {
            "car": "object",
            "red": {
              "element": "multimodal_property",
              "type": "color",
              "value": "red"
            }
          }
        },
        {
          "modality": "audio",
          "description": "a high-pitched scream",
          "breakdown": {
            "scream": "action",
            "high-pitched": {
              "element": "multimodal_property",
              "type": "pitch",
              "value": "high"
            }
          }
        }
      ]
    }
  }
}
