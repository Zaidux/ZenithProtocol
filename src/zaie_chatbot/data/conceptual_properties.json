{
  "conceptual_elements": {
    "agent": {
      "definition": "An agent is a living entity or autonomous system capable of performing actions.",
      "properties": [
        "is_autonomous",
        "has_intent",
        "can_perform_actions",
        "has_attributes"
      ],
      "examples": [...]
    },
    "object": {
      "definition": "A static entity with useful properties, often manipulated by an agent.",
      "properties": [
        "is_physical",
        "has_material",
        "has_function",
        "has_reason",
        "is_manipulable"
      ],
      "examples": [...]
    },
    "action": {
      "definition": "A process executed by an agent with a specific purpose or reason.",
      "properties": [
        "has_initiator",
        "has_purpose",
        "changes_state",
        "has_temporal_context"
      ],
      "examples": [...]
    },
    "motion": {
      "definition": "Something constant, a process already happening, or a state of being.",
      "properties": [
        "is_ongoing",
        "is_process",
        "describes_agent_state"
      ],
      "examples": [...]
    },
    "bridge": {
      "definition": "Relational words that connect conceptual elements and clarify relationships.",
      "properties": [
        "connects_elements",
        "clarifies_causality",
        "indicates_purpose"
      ],
      "examples": [...]
    },
    "properties": {
      "definition": "Foundational, detailed data that provides a profound conceptual understanding of the world.",
      "properties": [
        "is_foundational",
        "provides_causal_context",
        "defines_attributes"
      ],
      "examples": [...]
    },
    "multimodal_properties": {
      "definition": "Attributes of a concept derived from non-textual data.",
      "properties": [
        "visual_prop:color",
        "visual_prop:shape",
        "visual_prop:texture",
        "audio_prop:pitch",
        "audio_prop:tempo",
        "audio_prop:emotion"
      ],
      "examples": [
        {
          "visual_example": {
            "prompt": "A picture of a red car.",
            "breakdown": {
              "red_car": {
                "element": "multimodal_property",
                "properties": ["visual_prop:color", "visual_prop:shape"],
                "value": "red, car-like"
              }
            }
          }
        },
        {
          "audio_example": {
            "prompt": "An audio clip of a happy song.",
            "breakdown": {
              "happy_song": {
                "element": "multimodal_property",
                "properties": ["audio_prop:emotion", "audio_prop:tempo"],
                "value": "happy, fast"
              }
            }
          }
        }
      ]
    },
    "socio_linguistic_properties": {
      "definition": "Attributes of a concept that are context-dependent and relate to communication style.",
      "properties": [
        "tone",
        "formality",
        "sentiment",
        "style"
      ],
      "examples": [
        {
          "sentence": "That's just great.",
          "breakdown": {
            "great": {
              "element": "socio_linguistic_property",
              "attributes": ["tone:sarcastic", "sentiment:negative"],
              "purpose": "A negative tone for a seemingly positive word."
            }
          }
        }
      ]
    }
  }
}
